#include <bits/stdc++.h>
using namespace std;

int main(){
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int n; if(!(cin>>n)) return 0;
    vector<int> a(n);
    for(int i=0;i<n;++i) cin>>a[i];
    int X; cin>>X;
    int l=0, r=n-1;
    while(l<r){
        int s=a[l]+a[r];
        if(s==X){ cout<<l<<' '<<r<<"\n"; return 0;}
        else if(s<X) ++l;
        else --r;
    }
    cout<<"No\n";
    return 0;
}
