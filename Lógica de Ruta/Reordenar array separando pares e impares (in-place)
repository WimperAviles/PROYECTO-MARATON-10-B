#include <bits/stdc++.h>
using namespace std;

int main(){
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int n; if(!(cin>>n)) return 0;
    vector<int> a(n);
    for(int i=0;i<n;++i) cin>>a[i];
    int l=0, r=n-1;
    while(l<r){
        while(l<r && a[l]%2==0) ++l;
        while(l<r && a[r]%2!=0) --r;
        if(l<r) swap(a[l++], a[r--]);
    }
    for(int x:a) cout<<x<<' ';
    cout<<'\n';
    return 0;
}
