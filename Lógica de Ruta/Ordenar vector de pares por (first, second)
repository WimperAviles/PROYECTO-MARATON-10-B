#include <bits/stdc++.h>
using namespace std;

int main(){
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int n; if(!(cin>>n)) return 0;
    vector<pair<int,int>> v(n);
    for(int i=0;i<n;++i) cin>>v[i].first>>v[i].second;
    sort(v.begin(), v.end()); // by first then second
    for(auto &p: v) cout<<p.first<<','<<p.second<<' ';
    cout<<'\n';
    return 0;
}
